		System.out.print("문자 : ");
		String str2 = sc.next();
		
		char[] ar = new char[str1.length()];
		
		for(int i = 0; i < str1.length(); i++) {
			ar[i] = str1.charAt(i);
			
			
			
			
		}
		System.out.print(str1 + "에" + str2 +"가 존재하는 위치 (인덱스)");
		
		
		
		
	}

