package com.kh.practice1.func;

import java.util.Scanner;

public class Test17 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.print("정수 입력 : ");
		int num = sc.nextInt();

		for (int i = 1 ; 1 <= i; i++) {
			for (int j = num; 1 <= j; i--) {
				System.out.print('*');
			}
		}
		System.out.println();

	}

}
