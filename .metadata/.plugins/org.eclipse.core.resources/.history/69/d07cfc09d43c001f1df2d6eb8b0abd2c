package com.kh.practice1.func;

import java.util.Scanner;

public class Test23 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.print("정수 입력 : ");
		int num = sc.nextInt();

		for (int i = 0; i < num; i++) {
			System.out.println("*");
		}
		for (int i = 0; i < num - 1; i++) {
			System.out.print(" ");
			System.out.print("*");
		}

	}

}
