package com.kh.practice1.func;

import java.util.Scanner;

public class Test44 {
	public static void main(String []args) {
		
		Scanner sc = new Scanner(System.in);
		
		int[][] ar = new int [6][6];
		
		System.out.print("행 인덱스 입력 :");
		int row = sc.nextInt();
		System.out.print("열 인덱스 입력 :");
		int col = sc.nextInt();
		
		for(int i = 0; i < row; i++) {
			for (int j = 0; j < col; j++) {
				
				ar[i][j] = ar[i][j];
				System.out.print(ar[i][j]);
			}
			System.out.println();
		}
	}

}
