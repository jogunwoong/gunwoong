package com.kh.practice1.func;

import java.util.Scanner;

public class Test31 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.print("정수 : ");
		int num = sc.nextInt();
		int ar[] = new int[num];

		/* ============================================== */
		for (int i = 0; i <= num; i++) {
			System.out.print("배열 " + i + "번째 인덱스에 넣을 값 : ");
			ar[i] = sc.nextInt();
			System.out.println(ar[1]);
		

		/* ============================================== */

		
		}
	}

}
