package com.kh.practice1.func;

import java.util.Scanner;

public class Test1 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		System.out.println("정수 : ");
		int num = sc.nextInt();
		int[] ar = new int[num];
		
		for(int i = 0; i <= num; i++) {
			ar[num] = i;
		}

	}
}