package com.kh.practice1.func;

public class Test38 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String [][]arr = new String [3][3];
		 arr[0][0] = "(0, 0)";
		 arr[0][1] = "(0, 1)";
		 arr[0][2] = "(0, 2)";
		 arr[1][0] = "(1, 0)";
		 arr[1][1] = "(1, 1)";
		 arr[1][2] = "(1, 2)";
		 arr[2][0] = "(2, 0)";
		 arr[2][1] = "(2, 1)";
		 arr[2][2] = "(2, 2)";
		 System.out.print(arr);
	}

}
